FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
        default         Gauss none;
        default         cellMDLimited Gauss linear 1;
}

divSchemes
{
    default         none;
    div(phi,T)      Gauss MUSCL;
}

laplacianSchemes
{
    default         none;
    laplacian(DT,T) Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

